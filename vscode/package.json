{
  "name": "anki-notes",
  "displayName": "anki-notes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "anki-notes",
        "aliases": [
          "Anki Notes",
          "anki-notes"
        ],
        "extensions": [
          ".note"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "anki-notes",
        "scopeName": "source.anki-notes",
        "path": "./syntaxes/anki-notes.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "anki-notes.showPreview",
        "title": "Show Preview of Anki Notes"
      }
    ]
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/vscode": "^1.102.0",
    "typescript": "^5.9.2"
  }
}
